{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "snake",
  "version": "1.0.0",
  "identifier": "com.snake.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "decorations": false,
        "title": "snake",
        "width": 552,
        "height": 614
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
